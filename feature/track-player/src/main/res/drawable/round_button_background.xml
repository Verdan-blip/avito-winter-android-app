<?xml version="1.0" encoding="utf-8"?>
<selector android:shape="oval"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true">
        <shape android:shape="oval">
            <solid android:color="@color/material_dynamic_primary70" />
            <padding
                android:bottom="@dimen/padding_normal"
                android:left="@dimen/padding_normal"
                android:top="@dimen/padding_normal"
                android:right="@dimen/padding_normal" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="@color/material_dynamic_primary30" />
            <padding
                android:bottom="@dimen/padding_normal"
                android:left="@dimen/padding_normal"
                android:top="@dimen/padding_normal"
                android:right="@dimen/padding_normal" />
        </shape>
    </item>

</selector>
